<div class="table-responsive">
<table class="table table-striped table-bordered">
    <thead>
      <tr class="table-info">
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Tip</th>
        <th scope="col">Cijena</th>
        <th scope="col">Image</th>
      </tr>
    </thead>
    <tbody>
        <% if(list.length !== 0) {
          
          let i = 0
          list[list.length-1].forEach((art) => { %>
            <tr>
              <td><%= i+1 %> </td>
              <td><%= art.title%></td>
              <td><%= art.tip%> </td>
              <td><%= art.cijena%> </td>
              <td><img src= <%= art.slika%> alt="slika"></td> 
            </tr>

          <% i++ %>

            <% })} else {%>
                <h2>Loading...</h2>
            <% } %>
    </tbody>
  </table>
</div>